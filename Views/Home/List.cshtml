<!DOCTYPE html>
<html>
  <head>
    <meta charset='utf-8'>
    <title>Car List</title>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
  </head>
  <body>
    <div class="jumbotron">
      <h1>Here is your information</h1>
      @foreach (var adds in Model)
      {
    <div class="row">
    <div class="col-md-2">
      </div>
      <div class="col-md-2" align="left">
        <p>@adds.GetName()</p>
        <p>@adds.GetPhone()</p>
        <p>@adds.GetAddress()</p>
      </div>
    </div>
      }
    </div>
    <div class="well">
      <form action="/" method="post">
        <button type="submit" name="add">Add Another</button>
      </form>
      <form action="/list/clear" method="post">
        <button type="submit" name="clear">Clear List</button>
      </form>
    </div>

  </body>
</html>
